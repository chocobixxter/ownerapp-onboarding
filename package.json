{
  "name": "restaurant-location-analyzer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "start:full": "concurrently \"npm run server:start\" \"npm run dev\""
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0",
    "axios": "^1.6.0",
    "lucide-vue-next": "^0.294.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "radix-vue": "^1.4.0",
    "vaul-vue": "^0.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vue-tsc": "^1.8.0",
    "vite": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2"
  }
} 